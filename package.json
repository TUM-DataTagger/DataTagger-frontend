{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "fdm-frontend",
	"version": "0.0.0",
	"description": "",
	"private": true,
	"scripts": {
		"build": "pnpm --if-present --filter @fdm-frontend/* build",
		"build:check": "pnpm --if-present --filter @fdm-frontend/* build:check",
		"dev": "pnpm --parallel --if-present --filter @fdm-frontend/* dev",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"lint": "pnpm --filter @fdm-frontend/* lint",
		"format": "pnpm --filter @fdm-frontend/* format",
		"fmt": "pnpm --filter @fdm-frontend/* format",
		"prepare": "npx only-allow pnpm && pnpm exec is-ci || pnpm exec husky",
		"update": "pnpm --recursive update --interactive",
		"update:latest": "pnpm --recursive update --interactive --latest"
	},
	"type": "module",
	"keywords": [],
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@types/lodash.merge": "^4.6.9",
		"@vitest/coverage-istanbul": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"commitlint-config-jira": "^1.6.4",
		"commitlint-plugin-jira-rules": "^1.6.4",
		"eslint": "^9.30.1",
		"eslint-config-neon": "^0.2.7",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-react-compiler": "19.1.0-rc.2",
		"husky": "^9.1.7",
		"is-ci": "^4.1.0",
		"lint-staged": "^16.1.2",
		"lodash.merge": "^4.6.2",
		"prettier": "^3.6.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.35.1",
		"vitest": "^3.2.4"
	}
}
